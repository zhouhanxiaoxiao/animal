               animal.genotype                 as animal_genotype,
               animal.resource                 as animal_resource,
               animal.parent_id                as animal_parent_id,
               animal.createTime               as animal_createTime,
               animal.createUser               as animal_createUser,
               animal.selfIndex                as animal_selfIndex,
               animal.curstatu                 as animal_curstatu,
               animal.updateTime               as animal_updateTime,
               animal.updateUser                as animal_updateUser
        from cibr_task_detail_drosophila detail
                 left join cibr_stock_drosophila stock on stock.id = detail.stockid
                 left join cibr_animal_drosophila animal on animal.id = stock.drosophila_id
        where detail.askid = '${askId}'
        order by detail.confirmStatu, stock.stockIndex
    </select>
</mapper>