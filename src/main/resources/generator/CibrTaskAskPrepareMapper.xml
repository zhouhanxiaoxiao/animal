                 left join cibr_task_detail_drosophila detail on prepare.detailId = detail.id
                 left join cibr_stock_drosophila stock on stock.id = detail.stockid
                 left join cibr_animal_drosophila animal on stock.drosophila_id = animal.id
        where prepare.askid = '${askId}'
        order by prepare.prestatu
    </select>
</mapper>